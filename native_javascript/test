class Notify
  constructor:(@title,@options)->
  print:->
    console.log(this.title+this.options)
  launchNotiWindow =(title,options)->
    notify = new Notification(title,options)
  getPermissionAndLaunch=(title,options)->
    Notification.requestPermission (permission)->
      launchNotiWindow(title,options)
  launch:->
    getPermissionAndLaunch(this.title,this.options)